function DiskGetplusImage : boolean;
var
  IFile : file;
  FSize : word;
  R     : word;
begin
  DiskGetplusImage := false;
  assign(IFile,'plus.img                                                              ');
  {I-} reset(IFile,1); {$I+}
  if IOResult <> 0 then exit;
  FSize := FileSize(IFile);
  if MemAvail > FSize then begin
    reset(IFile,FSize);
    GetMem(plus,FSize);
    BlockRead(IFile,plus^,1,R);
    DiskGetplusImage := true;
  end;
  close(IFile);
end;

